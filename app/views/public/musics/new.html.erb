<div class="container">
      <div class="row">
        <%= form_with model: Music.new do |f| %>
        <div class="form-group">
          <%= f.label :音源をアップロード %><br />
          <%= f.file_field :audio %>
        </div>
        
          <div class="form-group">
            <%= f.label :タイトル %><br />
            <%= f.text_field :name %>
          </div>
          <div class="form-group">
            <%= f.label :説明 %><br />
            <%= f.text_field :explanation %>
          </div>
          <div class="form-group">
            <%= f.label :楽曲の画像 %><br />
            <%= f.file_field :image, accept: "image/*" %>
          </div>
        <div class="center-block">
          <%= f.submit '投稿する！', class:"btn btn-primary" %>
        </div>
        <% end %>
       </div>
  </div>
</div>
