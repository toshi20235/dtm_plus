<div class="container">
  <div><h3>新曲4選</h3></div>
  <div class="row mt-5 text-center">
    <% @musics.each do |music| %>
    <div class="col-3">
      <%= image_tag music.image, size: "250x250" %>
      <%= music.name %>
    </div>
    <% end %>
  </div>
  <div class="text-center mt-5">
    <%= link_to "ゲストログイン",users_guest_sign_in_path, method: :post, class:"btn btn-outline-info" %>
  </div>
</div>